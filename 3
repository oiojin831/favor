<ul>
  <% @users.each do |user| %>
    <li><%= user.email %></li>
    <% current_user.friends.each do |fr| %>
      <% unless (fr.id == user.id && current_user == user.id) %>
        <%= form_for :friendship, url: friendships_path do |f| %>
          <%= f.text_field :friend_id, value: user.id %>
          <%= f.submit %>
        <% end %>
      <% end %>
    <% end %>
  <% end %>
</ul>
